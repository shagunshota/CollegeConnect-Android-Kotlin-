                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_100ssp"
                            tools:ignore="MissingConstraints">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView2"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="20dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <TextView
                                        android:id="@+id/studentcount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="16dp"
                                        android:text="0"
                                        android:textSize="@dimen/_30ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/student"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/studentcount" />


                                </androidx.constraintlayout.widget.ConstraintLayout>


                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView3"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="16dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView2"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toEndOf="@+id/cardView2"
                                app:layout_constraintTop_toTopOf="@+id/cardView2"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <TextView
                                        android:id="@+id/teachercount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="16dp"
                                        android:text="0"
                                        android:textSize="@dimen/_30ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/teacher"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/teachercount" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="12dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView3"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.3"
                                app:layout_constraintStart_toEndOf="@+id/cardView3"
                                app:layout_constraintTop_toTopOf="@+id/cardView3"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <TextView
                                        android:id="@+id/admincount"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="16dp"
                                        android:text="0"
                                        android:textSize="@dimen/_30ssp"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/admin"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/admincount" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/constraintLayout2"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_100ssp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/constraintLayout"
                            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView21"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="20dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addstudent"
                                        android:layout_width="@dimen/_40ssp"
                                        android:layout_height="@dimen/_40ssp"
                                        android:layout_marginTop="16dp"
                                       android:src="@drawable/cardadd"

                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/student"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addstudent" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView31"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="16dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView21"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toEndOf="@+id/cardView21"
                                app:layout_constraintTop_toTopOf="@+id/cardView21"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addteacher"
                                        android:layout_width="@dimen/_40ssp"
                                        android:layout_height="@dimen/_40ssp"
                                        android:layout_marginTop="16dp"
                                        android:src="@drawable/cardadd"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/teacher"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addteacher" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="12dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView31"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.3"
                                app:layout_constraintStart_toEndOf="@+id/cardView31"
                                app:layout_constraintTop_toTopOf="@+id/cardView31"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addadmin"
                                        android:layout_width="@dimen/_40ssp"
                                        android:layout_height="@dimen/_40ssp"
                                        android:layout_marginTop="16dp"
                                        android:src="@drawable/cardadd"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="@string/admin"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addadmin" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>


                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_100ssp"
                            app:layout_constraintEnd_toEndOf="@+id/constraintLayout2"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="@+id/constraintLayout2"
                            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2">

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView211"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="20dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addevent"
                                        android:layout_width="@dimen/_40ssp"
                                        android:layout_height="@dimen/_40ssp"
                                        android:layout_marginTop="16dp"
                                        android:src="@drawable/cardadd"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="Event"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addevent" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardView311"
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="16dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView211"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.0"
                                app:layout_constraintStart_toEndOf="@+id/cardView211"
                                app:layout_constraintTop_toTopOf="@+id/cardView211"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addnotification"
                                        android:layout_width="@dimen/_40ssp"
                                        android:layout_height="@dimen/_40ssp"
                                        android:layout_marginTop="16dp"
                                        android:src="@drawable/cardadd"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="8dp"
                                        android:text="Notification"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addnotification" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>

                            <androidx.cardview.widget.CardView
                                android:layout_width="@dimen/_80ssp"
                                android:layout_height="@dimen/_80ssp"
                                android:layout_marginHorizontal="@dimen/_10ssp"
                                android:layout_marginStart="12dp"
                                app:cardCornerRadius="@dimen/_18ssp"
                                app:layout_constraintBottom_toBottomOf="@+id/cardView311"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.3"
                                app:layout_constraintStart_toEndOf="@+id/cardView311"
                                app:layout_constraintTop_toTopOf="@+id/cardView311"
                                app:layout_constraintVertical_bias="1.0" >
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:id="@+id/addfee"
                                        android:layout_width="@dimen/_30ssp"
                                        android:layout_height="@dimen/_30ssp"
                                        android:layout_marginTop="24dp"
                                        android:src="@drawable/fee"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.507"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toTopOf="parent" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginTop="16dp"
                                        android:text="Fee"
                                        android:textSize="@dimen/_10ssp"
                                        android:textStyle="bold"
                                        app:layout_constraintEnd_toEndOf="parent"
                                        app:layout_constraintHorizontal_bias="0.506"
                                        app:layout_constraintStart_toStartOf="parent"
                                        app:layout_constraintTop_toBottomOf="@+id/addfee" />


                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.cardview.widget.CardView>
                        </androidx.constraintlayout.widget.ConstraintLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


            </LinearLayout>

        </LinearLayout>








    </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>
</layout>